{% extends 'base.html.twig' %}

{% block title %}Fan
{% endblock %}

{% block main %}
	<main class="main-show">
		<h1>Fan</h1>

		<table class="table">
			<tbody>
				<tr>
					<th>Id</th>
					<td>{{ fan.id }}</td>
				</tr>
				<tr>
					<th>Marque</th>
					<td>{{ fan.marque }}</td>
				</tr>
				<tr>
					<th>Modele</th>
					<td>{{ fan.modele }}</td>
				</tr>
				<tr>
					<th>Prix</th>
					<td>{{ fan.prix }}</td>
				</tr>
				<tr>
					<th>Quantity</th>
					<td>{{ fan.quantity }}</td>
				</tr>
				<tr>
					<th>Width</th>
					<td>{{ fan.width }}</td>
				</tr>
			</tbody>
		</table>
		<div>
			<a href="{{ path('app_fan_index') }}">back to list</a>

			<a href="{{ path('app_fan_edit', {'id': fan.id}) }}">edit</a>

			{{ include('fan/_delete_form.html.twig') }}
		</div>
	</main>
{% endblock %}
